# Applied AI Research Agent Environment Variables
# Copy this file to .env and fill in your API keys

# Groq API key (required)
# Get your API key from https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Model selection
# Default: deepseek-r1-distill-llama-70b
# Other options: llama3-70b-8192, mixtral-8x7b-32768, gemma-7b-it
# GROQ_MODEL=llama3-70b-8192

# Agent configuration
# MAX_ITERATIONS=5
# EARLY_TERMINATION_THRESHOLD=90
# COMPLETION_THRESHOLD=80
# CATEGORY_THRESHOLD=16
# MIN_IMPROVEMENT_THRESHOLD=5

# Output configuration
# OUTPUT_DIR=./research_outputs
# SAVE_INTERIM_RESULTS=false 